[
    {
       "ImageName":"soelangen/pcc-nodejs",
       "LogoURL":"",
       "TemplateName":"NodeJS-pcc",
       "Inputs":[
          {
             "Label":"/data",
             "Default":"node_data",
             "Type":"VOLUME",
             "SecretName":"node_data"
          },
          {
             "Label":"Port1",
             "Default":"3000",
             "Type":"PORT",
             "PortContainer":"3000",
             "PortType":"tcp"
          }
       ],
       "ManifestBoilerplate":{
          "ManifestParameters":{
             "Files":{
                "/dev/attestation/keys/node_data":{
                   "Data":"{{ raw .Secrets.node_data.Private }}",
                   "Encoding":"string",
                   "NoTemplates":false
                }
             },
             "Argv":[
                "/usr/bin/node"
             ]
          },
          "ManifestSecrets":{
             "node_data":{
                "Type":"symmetric-key",
                "Size":128,
                "UserDefined":true
             }
          }
       }
    }
 ]