[
   {
      "ImageName":"soelangen/pcc-nodejs",
      "LogoURL":"https://upload.wikimedia.org/wikipedia/commons/d/d9/Node.js_logo.svg",
      "TemplateName":"NodeJS-pcc",
      "Inputs":[
         {
            "Label":"/data",
            "Default":"node_data",
            "Type":"VOLUME",
            "SecretName":"nodeData"
         },
         {
            "Label":"Port1",
            "Default":"3000",
            "Type":"PORT",
            "PortContainer":"3000",
            "PortType":"tcp"
         }
      ],
      "ManifestBoilerplate":{
         "ManifestParameters":{
            "Files":{
               "/dev/attestation/keys/node_data":{
                  "Data":"{{ raw .Secrets.nodeData.Private }}",
                  "Encoding":"string",
                  "NoTemplates":false
               }
            },
            "Argv":[
               "/usr/bin/node",
               "/data/app.js"
            ]
         },
         "ManifestSecrets":{
            "nodeData":{
               "Type":"symmetric-key",
               "Size":128,
               "UserDefined":true
            }
         }
      }
   }
]